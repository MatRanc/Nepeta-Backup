<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>title</key>
	<string>iOS Blocks</string>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Icons:</string>
			<key>footerText</key>
			<string>If a widget pushes icons onto the next page, this will attempt to move them back after closing a widget.</string>
		</dict>
		<dict>
			<key>PostNotification</key>
			<string>com.matchstic.ibk/settingschanged</string>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<false/>
			<key>defaults</key>
			<string>com.iosblocks.curago</string>
			<key>key</key>
			<string>shouldHideBadge</string>
			<key>label</key>
