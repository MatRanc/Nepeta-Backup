		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Lock widgets using a passcode.</string>
		</dict>
		<dict> 
              <key>cell</key> 
              <string>PSLinkListCell</string> 
              <key>detail</key>
              <string>IBKPasscodeController</string> 
              <key>get</key> 
              <string>passcodeLockEnabled:</string> 
              <key>id</key>
              <string>passcode</string> 
              <key>label</key> 
              <string>Passcode</string> 
              <key>mode</key>
              <integer>3</integer> 
              <key>pane</key> 
              <string>DevicePINPane</string> 
         </dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
