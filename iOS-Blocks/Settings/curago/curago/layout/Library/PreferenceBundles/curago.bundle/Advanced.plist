			<string>transparentWidgets</string>
			<key>label</key>
			<string>Transparent Background</string>
			<key>set</key>
			<string>setTransparentBackground:specifier:</string>
		</dict>
		<dict>
			<key>PostNotification</key>
			<string>com.matchstic.ibk/reloadallwidgets</string>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.iosblocks.curago</string>
			<key>key</key>
			<string>borderedWidgets</string>
			<key>label</key>
			<string>Show Border</string>
