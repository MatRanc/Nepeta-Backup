			<string>Move Icons Back After Closing</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Widgets:</string>
		</dict>
		<dict>
			<key>PostNotification</key>
			<string>com.matchstic.ibk/reloadallwidgets</string>
			<key>cell</key>
			<string>PSSwitchCell</string>
